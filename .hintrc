{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-webkit-mask-image"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not edge <= 112",
    "not ios_saf <= 16.4",
    "not safari <= 16.4",
    "not and_chr <= 112",
    "not chrome <= 112",
    "not opera <= 95",
    "not samsung <= 20"
  ]
}